{
  "name": "mindbird/contao-be-changelog",
  "description": "Anzeigen eines Changelogs auf der Backendstartseite",
  "minimum-stability": "stable",
  "license": "proprietary",
  "keywords":["contao", "changelog"],
  "type":"contao-module",
  "authors": [
    {
      "name": "mindbird",
      "email": "info@mindbird.de"
    }
  ],
  "require": {
    "php": ">=5.3.2",
    "contao/core": ">=3.2,<4"
  },
  "extra": {
    "contao": {
      "sources": {
        "contao": "system/modules/be-changelog"
      }
    }
  },
  "autoload": {
    "psr-4": {
      "BeChangelog\\": "src/"
    }
  },
  "replace": {
    "contao-legacy/be-changelog": "self.version"
  }
}